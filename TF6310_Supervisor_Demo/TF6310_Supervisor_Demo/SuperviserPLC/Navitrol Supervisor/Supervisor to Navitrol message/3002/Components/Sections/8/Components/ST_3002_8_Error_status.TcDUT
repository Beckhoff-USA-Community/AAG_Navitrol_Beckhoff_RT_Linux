<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4026.17">
  <DUT Name="ST_3002_8_Error_status" Id="{1fdfc678-69c3-4475-bc5f-442281a111a0}">
    <Declaration><![CDATA[{attribute 'pack_mode' := '1'}
TYPE ST_3002_8_Error_status :
STRUCT
	Communication_error         : BIT; // BMS has lost the communication to the battery. *
	Configuration_error         : BIT; // Battery has not been correctly configured. *
	Undervoltage                : BIT; // Undervoltage detected. Operating voltage is low. Prepare to charge or replace the battery. *
	Overvoltage                 : BIT; // Overvoltage detected. Overvoltage is a sustained voltage that exceeds the normal steady state limits. *
	Overcurrent                 : BIT; // Overcurrent detected. Might be caused by overloading or short circuiting the battery. *
	Temperature_error           : BIT; // The temperature of the battery is at unsafe levels. * In addition, Navitrol will set ERROR_BATTERY_HIGH_TEMPERATURE
	Internal_defect             : BIT; // The battery has a defect and might not work properly. *
	Bad_state_of_health         : BIT; // The battery is worn down in use. It might not fully charge anymore. *
	Faulty_charger              : BIT; // The charger connected to the battery is not working. *
	Faulty_contactor            : BIT; // The contactor of the battery is not working. *
	Current_sensor_defect       : BIT; // The current sensor of the battery is not working. *
	Temperature_sensor_defect   : BIT; // The temperature sensor of the battery is not working. *
	Battery_cell_drift          : BIT; // Two or more battery cells are in imbalance. Can lead to over-charging or over-discharging harming the battery. *
	Charging_error              : BIT; // Error while charging the battery *
	Generic_error               : BIT; // This can be used to indicate that there is some other battery related error that is not listed above. *
	Reserved                    : BIT; // 0 - Not in use
END_STRUCT
END_TYPE]]></Declaration>
  </DUT>
</TcPlcObject>